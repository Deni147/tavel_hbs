        {{#each reviews}}
        <div class="{{emoji}}">
        <div><h3>{{name}}</h3>
        <p>{{about}}</p>
        </div>
        <div><img src="/img/{{imgName}}" alt="{{name}}" height="150px"></div>
        {{!-- <p>{{emoji}}</p> --}}
        </div>
        {{/each}}


    

